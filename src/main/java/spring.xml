<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="triangle" class="com.zheng.spring.maven.springdemo.Triangle">
		<property name="points">
			<list>
				<ref bean="zeroPoint"/>
				<ref bean="point2"/>
				<ref bean="point3"/>
			</list>
		</property>
	</bean>
	
	<bean id="zeroPoint" class="com.zheng.spring.maven.springdemo.Point">
		<property name="x" value="0"/>
		<property name="y" value="0"/>
	</bean>
	
	<bean id="point2" class="com.zheng.spring.maven.springdemo.Point">
		<property name="x" value="-20"/>
		<property name="y" value="0"/>
	</bean>
	
	<bean id="point3" class="com.zheng.spring.maven.springdemo.Point">
		<property name="x" value="0"/>
		<property name="y" value="20"/>
	</bean>

</beans>