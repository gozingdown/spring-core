<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="triangle" class="com.zheng.spring.maven.springdemo.Triangle" autowire="byName" scope="singleton">
	</bean>
	
	<bean id="pointA" class="com.zheng.spring.maven.springdemo.Point">
		<property name="x" value="0"/>
		<property name="y" value="0"/>
	</bean>
	
	<bean id="pointB" class="com.zheng.spring.maven.springdemo.Point">
		<property name="x" value="-20"/>
		<property name="y" value="0"/>
	</bean>
	
	<bean id="pointC" class="com.zheng.spring.maven.springdemo.Point">
		<property name="x" value="0"/>
		<property name="y" value="20"/>
	</bean>
	
	<bean id="pointD" class="com.zheng.spring.maven.springdemo.Point">
		<property name="x" value="0"/>
		<property name="y" value="30"/>
	</bean>

</beans>